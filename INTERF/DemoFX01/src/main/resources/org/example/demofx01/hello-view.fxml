<VBox alignment="CENTER" prefHeight="538.0" prefWidth="763.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demofx01.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="74.0" prefWidth="259.0" />
    <Button onAction="#onHelloButtonClick" prefHeight="59.0" prefWidth="286.0" text="Hola">
      <font>
         <Font size="43.0" />
      </font></Button>

    <Button onAction="#onHelloButtonClick2" text="Boton Clave">
        <font>
            <Font size="43.0" />
        </font></Button>
</VBox>
